/-----------------------------------------------------------------------
/     FICHIER DES PARAMETRES DE SISYPHE
/-----------------------------------------------------------------------
BOUNDARY CONDITIONS FILE = geo_hippodrome.cli
GEOMETRY FILE            = geo_hippodrome.slf
RESULTS FILE             = gai_4NCOb_strat_vf.slf
FORTRAN FILE             = 'user_fortran2'
/-----------------------------------------------------------------------
/ PARAMETRES DE SORTIE
/-----------------------------------------------------------------------
VARIABLES FOR GRAPHIC PRINTOUTS =
'U,V,E,S,B,M,TOB,*ES,*S*,*A*'
MASS-BALANCE                    = YES
/-----------------------------------------------------------------------
/ PARAMETRES NUMERIQUES
/-----------------------------------------------------------------------
FINITE VOLUMES                  = YES
/-----------------------------------------------------------------------
/ PARAMETRES PHYSIQUES
/-----------------------------------------------------------------------
CLASSES TYPE OF SEDIMENT = NCO;NCO;NCO;NCO
CLASSES SEDIMENT DENSITY = 2650.;2650.;2650.;2650.
BED LOAD FOR ALL SANDS          = YES
SUSPENSION FOR ALL SANDS        = NO
BED-LOAD TRANSPORT FORMULA FOR ALL SANDS = 5
CLASSES SETTLING VELOCITIES = -9;-9;-9;-9
CLASSES SHIELDS PARAMETERS = -9;-9;-9;-9
CLASSES SEDIMENT DIAMETERS = 0.0002;0.00031;0.00103;0.00169
ACTIVE LAYER THICKNESS          = 0.03
/FRACTIONS FOR SIZE CLASS COMMENTED SINCE DEFINED IN USER FORTRAN
/CLASSES INITIAL FRACTION = 0.25;0.25;0.25;0.25
LAYERS NON COHESIVE BED POROSITY = 0.4;0.4;0.4
FORMULA FOR SLOPE EFFECT                     = 0
FORMULA FOR DEVIATION                        = 0
SEDIMENT SLIDE                               = NO
NUMBER OF LAYERS FOR INITIAL STRATIFICATION  = 3

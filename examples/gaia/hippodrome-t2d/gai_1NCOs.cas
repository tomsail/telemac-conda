/-----------------------------------------------------------------------
/     FICHIER DES PARAMETRES DE GAIA
/-----------------------------------------------------------------------
BOUNDARY CONDITIONS FILE = geo_hippodrome.cli
GEOMETRY FILE            = geo_hippodrome.slf
RESULTS FILE             = gai_1NCOs.slf
/DEBUGGER = 1
/-----------------------------------------------------------------------
/ PARAMETRES DE SORTIE
/-----------------------------------------------------------------------
VARIABLES FOR GRAPHIC PRINTOUTS =
'U,V,S,E,H,B,M,TOB,1A*,*ES,1M1,CS1'
MASS-BALANCE                    = YES
/---------------------------------------------------------------------
/ SUSPENSION T2D
/---------------------------------------------------------------------
SOLVER FOR DIFFUSION OF SUSPENSION   = 1
ACCURACY FOR DIFFUSION OF SUSPENSION = 1.D-12
MAXIMUM NUMBER OF ITERATIONS FOR SOLVER FOR SUSPENSION = 200
/---------------------------------------------------------------------
/ TYPE OF SEDIMENT
/---------------------------------------------------------------------
CLASSES TYPE OF SEDIMENT    = NCO
CLASSES SEDIMENT DIAMETERS  = 0.0002
CLASSES SETTLING VELOCITIES = -9
INITIAL SUSPENDED SEDIMENTS CONCENTRATION VALUES :0.D0
/---------------------------------------------------------------------
/ TRANSPORT
/---------------------------------------------------------------------
BED LOAD FOR ALL SANDS                   = NO
BED-LOAD TRANSPORT FORMULA FOR ALL SANDS = 5
/
SUSPENSION FOR ALL SANDS                 = YES
SUSPENSION TRANSPORT FORMULA FOR ALL SANDS = 1
SCHEME FOR ADVECTION OF SUSPENDED SEDIMENTS = 13
SCHEME OPTION FOR ADVECTION OF SUSPENDED SEDIMENTS = 1
/----------------------------------------------------------------------
/                     TURBULENCE
/----------------------------------------------------------------------
COEFFICIENT FOR DIFFUSION OF SUSPENDED SEDIMENTS = 1.E-6
/---------------------------------------------------------------------
/ SURFACE BED PARAMETERS
/---------------------------------------------------------------------
SKIN FRICTION CORRECTION = 0
ACTIVE LAYER THICKNESS = 0.01
FORMULA FOR SLOPE EFFECT                 = 0
FORMULA FOR DEVIATION                    = 0
SEDIMENT SLIDE                           = NO
/---------------------------------------------------------------------
/ BED MODEL without consolidation
/---------------------------------------------------------------------
BED MODEL = 1
/
NUMBER OF LAYERS FOR INITIAL STRATIFICATION = 1
LAYERS INITIAL THICKNESS = 1
CLASSES INITIAL FRACTION = 1.D0
/


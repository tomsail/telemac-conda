/-----------------------------------------------------------------------
/     FICHIER DES PARAMETRES DE GAIA
/-----------------------------------------------------------------------
BOUNDARY CONDITIONS FILE = geo_hippodrome.cli
GEOMETRY FILE            = geo_hippodrome.slf
RESULTS FILE             = gai_1COs_consolidation.slf
/-----------------------------------------------------------------------
/ PARAMETRES DE SORTIE
/-----------------------------------------------------------------------
VARIABLES FOR GRAPHIC PRINTOUTS =
'U,V,S,E,H,B,TOB,1A*,*ES,1M1'
MASS-BALANCE                    = YES
/---------------------------------------------------------------------
/ SUSPENSION T3D
/---------------------------------------------------------------------
ADVECTION-DIFFUSION SCHEME WITH SETTLING VELOCITY = 1
SOLVER FOR DIFFUSION OF SUSPENSION   = 1
ACCURACY FOR DIFFUSION OF SUSPENSION = 1.D-12
MAXIMUM NUMBER OF ITERATIONS FOR SOLVER FOR SUSPENSION = 200
/---------------------------------------------------------------------
/ TYPE OF SEDIMENT
/---------------------------------------------------------------------
CLASSES TYPE OF SEDIMENT    = CO
CLASSES SEDIMENT DIAMETERS  = 0.00001
CLASSES SETTLING VELOCITIES = 0.001
INITIAL SUSPENDED SEDIMENTS CONCENTRATION VALUES = 0.D0
/---------------------------------------------------------------------
/ TRANSPORT
/---------------------------------------------------------------------
BED LOAD FOR ALL SANDS                   = NO
BED-LOAD TRANSPORT FORMULA FOR ALL SANDS = 5
/
SUSPENSION FOR ALL SANDS                 = NO
SUSPENSION TRANSPORT FORMULA FOR ALL SANDS = 1
SCHEME FOR ADVECTION OF SUSPENDED SEDIMENTS = 13
SCHEME OPTION FOR ADVECTION OF SUSPENDED SEDIMENTS = 1
/----------------------------------------------------------------------
/                     TURBULENCE
/----------------------------------------------------------------------
COEFFICIENT FOR HORIZONTAL DIFFUSION OF SUSPENDED SEDIMENTS = 0.01D0
COEFFICIENT FOR VERTICAL DIFFUSION OF SUSPENDED SEDIMENTS = 1.E-6
/---------------------------------------------------------------------
/ SURFACE BED PARAMETERS
/---------------------------------------------------------------------
SKIN FRICTION CORRECTION = 0
ACTIVE LAYER THICKNESS = 0.01
FORMULA FOR SLOPE EFFECT                 = 0
FORMULA FOR DEVIATION                    = 0
SEDIMENT SLIDE                           = NO
/---------------------------------------------------------------------
/ BED MODEL with consolidation
/---------------------------------------------------------------------
BED MODEL = 2
/
NUMBER OF LAYERS FOR INITIAL STRATIFICATION = 4
LAYERS INITIAL THICKNESS                    = 0.25;0.25;0.25;0.25
CLASSES INITIAL FRACTION                    = 1.D0
/
LAYERS MUD CONCENTRATION                        = 50.D0;100.D0;200.D0;
300.D0
LAYERS CRITICAL EROSION SHEAR STRESS OF THE MUD = 0.1;0.2;0.3;0.4
LAYERS PARTHENIADES CONSTANT                    = 1.E-4; 1.E-4; 1.E-4;
1.E-4
LAYERS MASS TRANSFER                            = 0.01;0.005;0.001;0.0D0

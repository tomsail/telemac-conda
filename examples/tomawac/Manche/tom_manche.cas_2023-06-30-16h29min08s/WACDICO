CONTROLE = 0;2
DEFAUT = 0
DEFAUT1 = 0
CHOIX = '0="Pas dissipation par moutonnement"';
'1="Dissipation selon WAM cycle 4"';
'2="Dissipation selon Van des Westhuysen(2007)"'

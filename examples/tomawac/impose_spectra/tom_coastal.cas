/*------------------------------------------------------------------*/
/* WRITING SPECTRA                                                  */
/*------------------------------------------------------------------*/
PUNCTUAL RESULTS FILE = './spe_CoastalResults.slf'
FILE WITH COORDINATES OF SPECTRA TO WRITE =
'./tom_SpectraOutputOceanic.dat'
